  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@types/react": "^16.7.6",
    "@types/react-native": "^0.57.8",
    "@types/react-navigation": "^2.13.1",
    "@types/styled-components": "^4.1.0",
    "@types/yup": "^0.26.2",
    "apollo-codegen": "^0.20.2",
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.46.0",
